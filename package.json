{
  "name": "jetmoose",
  "dependencies": {
    "nuxt": "^2.0.0",
    "@nuxt/typescript-runtime": "^0.4.0",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^3.0.0-0",
    "@nuxtjs/dotenv": "^1.4.0"
  },
  "devDependencies": {
    "@webpack-cli/init": "^0.3.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "commitizen": "^4.0.4",
    "css-loader": "^3.5.2",
    "cz-conventional-changelog": "3.1.0",
    "husky": "^4.2.5",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.4",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^7.0.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^4.0.0-beta.8",
    "workbox-webpack-plugin": "^5.1.2",
    "@nuxt/typescript-build": "^0.6.0",
    "@nuxtjs/tailwindcss": "^1.0.0",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-jest": "^24.1.0",
    "jest": "^24.1.0",
    "vue-jest": "^4.0.0-0",
    "ts-jest": "^25.0.0"
  },
  "version": "0.0.2",
  "description": "Modern boilerplate for wordpress theme",
  "keywords": [
    "jetmoose",
    "wordpress",
    "theme",
    "vue",
    "boilerplate",
    "webpack"
  ],
  "author": "Epistol",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build": "nuxt-ts build",
    "commit": "npx git-cz",
    "dev": "nuxt-ts",
    "generate": "nuxt-ts generate",
    "start": "nuxt-ts start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "bugs": {
    "url": "https://github.com/Epistol/JetMoose/issues"
  },
  "homepage": "https://github.com/Epistol/JetMoose#readme"
}
